#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int x,y;
	    cin>>x>>y;
	     if((y-x)%8!=0)
	    cout<<1+(y-x)/8<<endl;
	    else if((y-x)%8==0)
	    cout<<(y-x)/8<<endl;
	    else if((y-x)<=8)
	    cout<<1<<endl;
	    else
	    cout<<0<<endl;
	    
	}
	return 0;
}
