#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n,vote_age,ppl_age,count=0;
        cin>>n>>vote_age;
        for(int i = 0;i<n;i++){
            cin>>ppl_age;
            if(ppl_age>=vote_age) count++;
        };
        cout<< count <<endl;
    }
	// your code goes here
	return 0;
}
